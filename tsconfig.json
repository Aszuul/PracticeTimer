{
	"compilerOptions": {
	   "lib": [
		  "es5",
		  "es6",
		  "DOM"
	   ],
	   "target": "es6",
	   "module": "none",
	   "moduleResolution": "node",
	   "outDir": "./dist",
	   "emitDecoratorMetadata": true,
	   "experimentalDecorators": true,
	   "sourceMap": true,
	   "strict": true,
	   // "noUnusedLocals":true,
	   "noImplicitAny": true,
	   "downlevelIteration": true
	},
	"exclude": [
		"./Tests/"
		,"./node_modules/"
		,"./dist/"
	]
 }